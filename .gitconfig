[user]
	name = SquidEpps
	email = squidepps@yandex.ru
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	di = difftool
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	visual = !gitk
	last = log -1 HEAD
	up = pull
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	editor = vim 
